{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist",
    "target": "es2019",
    "esModuleInterop": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,          
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}